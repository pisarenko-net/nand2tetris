class Main {
	function void main() {
		var World world;

		let world = World.new();

		do Game.displayWelcomeScreen();
		do Game.createLife(world);
		do Game.displayInstructions();

		do Editor.start(world);

		while (true) {
			do world.transformToNextGeneration();
		}

		do world.dispose();
		
		return;
	}
}